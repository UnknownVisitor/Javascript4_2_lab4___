entrypoint="index.html"
hidden=[".config"]
modules = ["nodejs-20", "nix"]

[nix]
channel = "stable-25_05"

[deployment]
publicDir = "/"
deploymentTarget = "static"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "WebServer"

[[workflows.workflow]]
name = "WebServer"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node index.js"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5069
externalPort = 3002

[[ports]]
localPort = 5076
externalPort = 3003

[[ports]]
localPort = 38537
externalPort = 3001

[[ports]]
localPort = 40667
externalPort = 3000

[agent]
expertMode = true
